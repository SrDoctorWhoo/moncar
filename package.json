{
    "name": "moncar-root",
    "version": "1.0.0",
    "description": "Root package to proxy commands to frontend",
    "private": true,
    "scripts": {
        "dev": "npm run dev --prefix frontend",
        "build": "npm run build --prefix frontend",
        "start": "npm run start --prefix frontend",
        "install": "npm install --prefix frontend",
        "prisma:generate": "npm run prisma:generate --prefix frontend",
        "prisma:push": "npm run prisma:push --prefix frontend",
        "prisma:seed": "npm run prisma:seed --prefix frontend"
    }
}